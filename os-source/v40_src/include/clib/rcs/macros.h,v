@#ifndef CLIB_MACROS_H
#define CLIB_MACROS_H
/*
** 	$Id: macros.h,v 1.1 90/07/12 20:28:41 kevin Exp $
**
**	C prototypes
**
**	(C) Copyright 1990 Commodore-Amiga, Inc.
**	    All Rights Reserved
*/

#define MAX(a,b)    ((a)>(b)?(a):(b))
#define MIN(a,b)    ((a)<(b)?(a):(b))
#define ABS(x)      ((x<0)?(-(x)):(x))

#endif	/* CLIB_MACROS_H */
@
