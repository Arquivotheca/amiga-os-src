    MSG_EDIT_RESTORE,

    MSG_OPTIONS_MENU,
    MSG_OPTIONS_SAVE_ICONS,

    MSG_SAVE_GAD,
    MSG_USE_GAD,
    MSG_CANCEL_GAD,
    MSG_OK_GAD,

    MSG_NORMAL_STAT,
    MSG_NO_MEMORY_STAT,
    MSG_IFF_UNKNOWNCHUNK_STAT,
    MSG_IFF_UNKNOWNVERSION_STAT,
    MSG_IFFERROR_STAT,
    MSG_DOSERROR_STAT,

    /* editor-specific strings */
    MSG_NO_LANGUAGES_STAT = PRIVATE_STRINGS,
    MSG_NO_COUNTRIES_STAT,

    MSG_REQ_LOAD,
    MSG_REQ_SAVE,

    MSG_AVAILLANG_GAD,
    MSG_PREFLANG_GAD,
    MSG_CLEARLANG_GAD,
    MSG_DAYLIGHTSAVINGS_GAD,
    MSG_COUNTRY_GAD,
    MSG_TIMEZONE_BOX,

    MSG_TZ0,
    MSG_TZ1,
