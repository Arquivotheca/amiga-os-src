#include <exec/types.h>


#define PRIVATE_STRINGS 4000


enum AppStringsID
{
    /* common strings that must be here */
    MSG_NOTHING,
    MSG_PROGRAM_ERROR,
    MSG_NO_ASL_LIBRARY,
    MSG_NO_IFFPARSE_LIBRARY,

    MSG_PROJECT_MENU,
    MSG_PROJECT_OPEN,
    MSG_PROJECT_SAVE_AS,
    MSG_PROJECT_QUIT,

