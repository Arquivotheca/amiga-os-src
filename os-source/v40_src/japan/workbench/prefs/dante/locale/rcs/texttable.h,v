    MSG_TZ16,
    MSG_TZ17,
    MSG_TZ18,
    MSG_TZ19,
    MSG_TZ20,
    MSG_TZ21,
    MSG_TZ22,
    MSG_TZ23
};

#define LAST_COMMON MSG_DOSERROR_STAT


struct LocaleInfo
{
    APTR li_LocaleBase;
    APTR li_Catalog;
};


STRPTR GetString(struct LocaleInfo *li, enum AppStringsID id);
