;   GetA0.a
;
;   Mitchell/Ware Systems           Version 1.00            30-Mar-89
;
;   Return the contents of A0 (or the other registers!)

    SECTION CODE

    XDEF    _GetA0
    XDEF    _GetA1
    XDEF    _GetA4
    XDEF    _GetA5
    XDEF    _GetA7

_GetA0: MOVE.L  A0,D0
        RTS

_GetA1: MOVE.L  A1,D0
        RTS

_GetA4: MOVE.L  A4,D0
        RTS

_GetA5: MOVE.L  A5,D0
        RTS

_GetA7: MOVE.L  A7,D0
        RTS
 
    END
    