failat 20
;; Check/Create directory structure
if not exists [Root$TS:]OS
    makedir [Root$TS:]OS
endif
if not exists [Root$TS:]OS/dos
    makedir [Root$TS:]OS/dos
endif
if not exists [Root$TS:]OS/dos/semacise
    makedir [Root$TS:]OS/dos/semacise
endif
if not exists [Root$TS:]OS/dos/semacise/src
    makedir [Root$TS:]OS/dos/semacise/src
endif
if not exists [Root$TS:]OS/dos/semacise/doc
    makedir [Root$TS:]OS/dos/semacise/doc
endif
if not exists [Root$TS:]OS/dos/semacise/bin
    makedir [Root$TS:]OS/dos/semacise/bin
endif

;; Copy directories h/, m/, c/
failat 21
copy #? to [Root$TS:][Path$OS/dos/semacise]/src CLONE
copy h/ to [Root$TS:][Path$OS/dos/semacise]/src/h ALL CLONE
copy m/ to [Root$TS:][Path$OS/dos/semacise]/src/m ALL CLONE
copy c/ to [Root$TS:][Path$OS/dos/semacise]/src/c ALL CLONE
copy d/ to [Root$TS:][Path$OS/dos/semacise]/src/d ALL CLONE
copy o/ to [Root$TS:][Path$OS/dos/semacise]/src/o ALL CLONE
copy mo/ to [Root$TS:][Path$OS/dos/semacise]/src/mo ALL CLONE
copy do/ to [Root$TS:][Path$OS/dos/semacise]/src/do ALL CLONE
copy docs/ to [Root$TS:][Path$OS/dos/semacise]/doc ALL CLONE

;; Generate .index files
AD PATS NoFF m/Semacise.c TO [Root$TS:][Path$OS/dos/semacise]/doc/Semacise.index
