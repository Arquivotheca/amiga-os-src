enum
{
	STANDARD_CYCLE,		/* current is standard FS */
	CUSTOM_CYCLE,		/* current is custom FS	  */
};

enum
{
	FSBS_512_CYCLE,		/* file system block size 512   */
	FSBS_1024_CYCLE,	/* file system block size 1024  */
	FSBS_2048_CYCLE,	/* file system block size 2048  */
	FSBS_4096_CYCLE,	/* file system block size 4096  */
	FSBS_8192_CYCLE,	/* file system block size 8192  */
	FSBS_16384_CYCLE,	/* file system block size 16384 */
	FSBS_32768_CYCLE,	/* file system block size 32768 */
};

