@;------ included files ------

	INCLUDE		"exec/types.i"
	INCLUDE		"exec/nodes.i"
	INCLUDE		"libraries/diskfont.i"
	INCLUDE		"14_rev.i"

;------ code to handle execution of this file ------
		moveq	#100,D0
		rts

;------ DiskFontHeader structure ------
		dc.l	0		; ln_Succ
		dc.l	0		; ln_Pred
		dc.b	NT_FONT		; ln_Type
		dc.b	0		; ln_Pri
		dc.l	fontName	; ln_Name
		dc.w	DFH_ID		; FileID
		dc.w	0		; Revision
		dc.l	0		; Segment
fontName:
		dc.b	0,'$VER: sapphire'
		VERS
		dc.b	' ('
		DATE
		dc.b	')',0
		dcb.b	MAXFONTNAME+8-(*-fontName),0

;------ TextFont structure ------
		dc.b	NT_FONT		; ln_Type
		dc.b	0		; ln_Pri
		dc.l	fontName	; ln_Name
		dc.l	0		; mn_ReplyPort
		dc.w	0		; (graphics library internal use)
		dc.w	14		; tf_YSize
		dc.b	$00		; tf_Style
		dc.b	$62		; tf_Flags
		dc.w	11		; tf_XSize
		dc.w	10		; tf_Baseline
		dc.w	1		; tf_BoldSmear
		dc.w	0		; tf_Accessors
		dc.b	32		; tf_LoChar
		dc.b	255		; tf_HiChar
		dc.l	font14Data	; tf_CharData
		dc.w	FONT14MODULO	; tf_Modulo
		dc.l	font14Loc		; tf_CharLoc
		dc.l	font14Space	; tf_CharSpace
		dc.l	font14Kern	; tf_CharKern


