desc
@helvetica/15 definition
@


36.5
log
@Fixed the definition of the space character with the fcom2asm tool.
@
text
@;------ included files ------

	INCLUDE		"exec/types.i"
	INCLUDE		"exec/nodes.i"
	INCLUDE		"libraries/diskfont.i"
	INCLUDE		"15_rev.i"

;------ code to handle execution of this file ------
		moveq	#100,D0
		rts

;------ DiskFontHeader structure ------
		dc.l	0		; ln_Succ
		dc.l	0		; ln_Pred
		dc.b	NT_FONT		; ln_Type
		dc.b	0		; ln_Pri
		dc.l	font15Name	; ln_Name
		dc.w	DFH_ID		; FileID
		dc.w	0		; Revision
		dc.l	0		; Segment
font15Name:
		dc.b	0,'$VER: helvetica'
		VERS
		dc.b	' ('
		DATE
		dc.b	')',0
		dcb.b	MAXFONTNAME+8-(*-font15Name),0

;------ TextFont structure ------
		dc.b	NT_FONT		; ln_Type
		dc.b	0		; ln_Pri
		dc.l	font15Name	; ln_Name
		dc.l	0		; mn_ReplyPort
		dc.w	0		; (graphics library internal use)
		dc.w	15		; tf_YSize
		dc.b	$00		; tf_Style
		dc.b	$62		; tf_Flags
		dc.w	8		; tf_XSize
		dc.w	11		; tf_Baseline
		dc.w	1		; tf_BoldSmear
		dc.w	0		; tf_Accessors
		dc.b	32		; tf_LoChar
		dc.b	255		; tf_HiChar
		dc.l	font15Data	; tf_CharData
		dc.w	FONT15MODULO	; tf_Modulo
		dc.l	font15Loc	; tf_CharLoc
		dc.l	font15Space	; tf_CharSpace
		dc.l	font15Kern	; tf_CharKern


