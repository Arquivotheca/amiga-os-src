text
@/*
	Include file for Tektronics_4693D driver.
	Bill Koester - April/88.
*/

/* COMMUNICATION OPTIONS */
#define  HANDSHAKE      0xc1
#define  DATA_STREAMING 0xc2
/* PIXEL SIZING */
#define  PIXEL_DEF      0xc0
#define  PIXEL_REC      0xc1
/* PIXEL ENCODING */
#define  TEK_1          0xc4
#define  TEK_2          0xc5
#define  TEK_4          0xc6
#define  TEK_8          0xc7
#define  INDEX_8        0xc8
#define  INDEX_12       0xc9
#define  ARBITRARY_2    0xcb
#define  ARBITRARY_4    0xcc
#define  ARBITRARY_8    0xcd
/* PIXEL ORDERING */
#define  ORDER_1        0xc1
#define  ORDER_2        0xc2
/* IMAGE SIZING */
#define  SIZE_DEF       0xc0
#define  SIZE_SAME      0xc1
#define  SIZE_REP       0xc2
#define  SIZE_INTERP    0xc3
/* IMAGE RENDERING */
#define  RENDER_DEF     0xc0
#define  RENDER_8       0xc1
#define  RENDER_FC1     0xc3
#define  RENDER_FC2     0xc4
#define  RENDER_BW      0xd1
#define  RENDER_GREY    0xd3
/* COLOR CONVERSION */
#define  COLOR_DEF      0xc0
#define  COLOR_NONE     0xc1
#define  COLOR_GAMA2_8  0xc8
#define  COLOR_GAMA2_2  0xca
#define  COLOR_GAMA2_4  0xcc
#define  COLOR_GAMA2_6  0xce
/* COLOR MANIPULATION */
#define  MANIP_DEF      0xc0
#define  MANIP_NONE     0xc1
#define  MANIP_MATCH    0xc2
#define  MANIP_GAMA     0xc3
/* BLACK WHITE INVERSION */
#define  BW_DEF         0xc0
#define  BW_NONE        0xc1
#define  BW_INVERT      0xc2
#define  BW_EXCH        0xc3
/* HARDCOPY ORIENTATION */
#define  ORIENT_DEF     0xc0
#define  ORIENT_LAND    0xc1
#define  ORIENT_BOT     0xc2
#define  ORIENT_CEN     0xc3
#define  ORIENT_TOP     0xc4
/* COPIER IDENTIFICATION */
#define  COPIER_NULL    0xc080
#define  COPIER_4693D   0xc995
@


