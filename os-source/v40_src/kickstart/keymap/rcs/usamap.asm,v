**	$Header: ramlib.asm,v 36.0 89/02/17 13:33:42 kodiak Exp $
d11 1
a11 1
	SECTION		keymap
d13 2
a14 1
*------ Included Files -----------------------------------------------
a15 2
	INCLUDE		"exec/types.i"
	INCLUDE		"devices/keymap.i"
d17 1
a17 1
*------ Exported Names -----------------------------------------------
d19 10
a28 2
	XDEF		_USANode
	XDEF		_USA1Node
a29 12
_USANode:
		DC.L	0,0		; ln_Succ, ln_Pred
		DC.B	0,0		; ln_Type, ln_Pri
		DC.L	USAName		; ln_Name
		DC.L	USALowMapType
		DC.L	USALowMap
		DC.L	USALCapsable
		DC.L	USALRepeatable
		DC.L	USAHighMapType
		DC.L	USAHighMap
		DC.L	USAHCapsable
		DC.L	USAHRepeatable
a30 15

