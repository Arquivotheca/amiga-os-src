#define MAX(a,b)        ((a)>(b)?(a):(b))
#define MIN(a,b)        ((a)<(b)?(a):(b))
#define ABS(x)  ((x<0)?(-(x)):(x))

#define SHORTMIN(a,b)	(SHORT)(MIN((SHORT)(a),(SHORT)(b)))
#define SHORTMAX(a,b)	(SHORT)(MAX((SHORT)(a),(SHORT)(b)))
