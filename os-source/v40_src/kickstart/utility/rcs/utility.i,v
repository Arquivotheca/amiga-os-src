	DC.B 'utility.library',0
	ENDM

   STRUCTURE UtilityBase,LIB_SIZE
   	UBYTE ub_Language
   	UBYTE ub_Reserved

;---------------------------------------------------------------------------

	ENDC	; UTILITY_UTILITY_I
