*------ 68000 Reset Vector -------------------------------------------
d78 2
a79 2
		DC.W    JMPINSTR
		DC.L    ColdStart	; WARNING: Must be $FC00D2 (see
d82 1
a82 1
*------ Manufacturing Diagnostic Pattern  ----------------------------
d84 1
a84 1
		DC.L    $0FFFF
d86 1
a87 2
*------ Kickstart release number, inserted by the build process. -----

d91 1
d93 2
a94 1
*------ Exec Release Numbers (Binary) --------------------------------
d96 2
a97 2
VerRev:		DC.W    VERSION
		DC.W    REVISION
d99 1
a99 2
VERNUM		EQU     VERSION
REVNUM		EQU     REVISION
d101 1
d103 1
a103 1
*------ System Serial Number -----------------------------------------
d105 3
a107 1
		DC.L    -1
d109 1
a109 1
*------ Version String -----------------------------------------------
d112 2
d115 1
a115 2
		CNOP	0,4		;Attempt to align RESET
					;jump to $F800D0
d117 17
a133 3
		;Ugly Kludge to get past Metacomco assembler limitations
ExecTest1:
		IFNE	ExecTest1-ExecOrigin-$30
@


