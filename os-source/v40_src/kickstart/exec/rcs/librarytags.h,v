#define	OLTAG_WORKBENCH	10

#define	OLTAG_COPYRIGHT1	(-1)	/* 'AMIGA ROM Operating System and Libraries',0	*/
#define	OLTAG_COPYRIGHT2	(-2)	/* 'Copyright © 1985-1992 ',0			*/
#define	OLTAG_COPYRIGHT3	(-3)	/* 'Commodore-Amiga, Inc. ',0			*/
#define	OLTAG_COPYRIGHT4	(-4)	/* 'All Rights Reserved.',0			*/
#define	OLTAG_COPYRIGHT5	(-5)	/* '3.00 ROM ',0					*/

#define	ODTAG_TIMER	0
#define	ODTAG_INPUT	1

#endif
@


