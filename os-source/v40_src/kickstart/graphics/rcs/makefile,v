37.2
log
@No more audhard.h crap.
@
text
@##########################################################################
#
# $Id: Makefile,v 37.1 91/01/04 19:20:31 spence Exp Locker: spence $
#
# $Locker: spence $
#
##########################################################################

VERSION=	37
SRCDIRPATH=     kickstart
V=              /usr/commodore/amiga/V${VERSION}
MAKEMETA=	${V}/tools/makemeta
MAKEFILE=	Makefile
SUBDIRS=	a c d gels text fonts
SRCDIR=		graphics
SUBSYSNAME=	graphics
SUBSYSGROUP=	graphics

GFXLIBS= \
a/a.lib \
c/c.lib \
d/d.lib \
gels/gels.lib \
text/text.lib \
fonts/fonts.lib

#DEBUG=		${V}/lib/debug.lib

MYLIBS=	${GFXLIBS} ${DEBUG} ${V}/lib/rom.lib

AFILES=	pwrup.asm

EXTERNIFILES= \
clip.i \
copper.i \
display.i \
gels.i \
gfx.i \
gfxbase.i \
gfxnodes.i \
layers.i \
monitor.i \
rastport.i \
regions.i \
sprite.i \
view.i \
displayinfo.i \
videocontrol.i

IFILES= \
cia8520.i \
sane_names.i \
macros.i \
displayinfo_internal.i \
${EXTERNIFILES}

EXTERNHFILES= \
clip.h \
collide.h \
copper.h \
display.h \
displayinfo.h \
gels.h \
gfx.h \
gfxbase.h \
gfxmacros.h \
gfxnodes.h \
graphint.h \
layers.h \
monitor.h \
rastport.h \
regions.h \
sprite.h \
view.h \
videocontrol.h

HFILES= \
cia8520.h \
sane_names.h \
macros.h \
displayinfo_internal.h \
${EXTERNHFILES}

FIRSTO=	pwrup.obj

MFLAGS=	VERSION=${VERSION}

LFLAGS=	XREF=$*.xref MAP=$*.lmap

#all:	graphics.doc
all:	${SUBDIRS} ${SUBSYSNAME}.ld

pwrup.obj:	graphics_rev.i

a/a.lib:
	cd a; make ${MFLAGS}

c/c.lib:
	cd c; make ${MFLAGS}

gels/gels.lib:
	cd gels; make ${MFLAGS}

text/text.lib:
	cd text; make ${MFLAGS}

fonts/fonts.lib:
	cd fonts; make ${MFLAGS}

.INCLUDE=${MAKEMETA}
@


