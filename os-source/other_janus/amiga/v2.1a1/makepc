echo "Copying PC/System dir"
copy system df0:pc/system quiet
copy system df0:+pc/system quiet

echo "Copying Fonts dir"
copy fonts df0:fonts all quiet

echo "Copying AMouse"
copy amouse/amouse.ld df0:pc/AMouse

echo "Copying Flipper"
copy flipper/src/flipper df0:PC/Flipper

echo "Copying janus.library"
copy jlib/janus.library df0:expansion/janus.library

echo "Copying PCDisk"
copy pcdisk/pcdisk.ld df0:pc/PCDisk

echo "Copying PCWindow"
copy pcwindow/zaphod.ld df0:pc/PCWindow

echo "Copying pc.boot"
copy sys:pc/system/pc.boot df0:pc/system/pc.boot
copy sys:pc/system/pc.boot df0:+pc/system/pc.boot
