text
@;------ included files ------

	INCLUDE		"exec/types.i"
	INCLUDE		"exec/nodes.i"

	XDEF	_compact31TextFont

font31Name:
		dc.b	"compact.font",0,0  ; 2 0s, to align things

;------ TextFont structure ------
_compact31TextFont
		dc.l	0		; ln_Succ
		dc.l	0		; ln_Pred
		dc.b	NT_FONT		; ln_Type
		dc.b	0		; ln_Pri
		dc.l	font31Name	; ln_Name
		dc.l	0		; mn_ReplyPort
		dc.w	0		; (graphics library internal use)
		dc.w	31		; tf_YSize
		dc.b	$00		; tf_Style
		dc.b	$62		; tf_Flags
		dc.w	25		; tf_XSize
		dc.w	24		; tf_Baseline
		dc.w	1		; tf_BoldSmear
		dc.w	0		; tf_Accessors
		dc.b	32		; tf_LoChar
		dc.b	254		; tf_HiChar
		dc.l	font31Data	; tf_CharData
		dc.w	FONT31MODULO	; tf_Modulo
		dc.l	font31Loc	; tf_CharLoc
		dc.l	font31Space	; tf_CharSpace
		dc.l	font31Kern	; tf_CharKern


