1.1
log
@Initial revision
@
text
@d1 1
a1 100
; :ts=8
*
*   Falselib.a -- Start up libprog without adding library
*
*   William A. Ware         AC30
*

    include 'exec/types.i'
    include 'exec/resident.i'
    include 'exec/nodes.i'
    include 'exec/libraries.i'
    include 'exec/resident.i'

    include "playerprefsbase.i"
    xref    __LibFuncTab    ; function table for library
    xref    __LibInit   ; start code for library
    xref    __LibExpunge

LSYSBASE    MACRO
            movea.l (4).w,a6
        ENDM

        xref    _LIBDoPlayer
        xref    _LIBDoPrefs
        xref    _LVOCloseLibrary
        xref    _LVOMakeLibrary
        xref    _LVOAddLibrary

        section text,CODE

_FalseLib:  
        movem.l d1/a0-a2,-(sp)
        movea.l (4).w,a6
        lea     __LibFuncTab,a0
        moveq   #0,d1
        movea.l d1,a1
        movea.l d1,a2
        move.l  #PlayerPrefsBase_SIZE,d0
        jsr     _LVOMakeLibrary(a6)
        tst.l   d0
        beq.s   0$
        movea.l d0,a2
        movea.w #1,a0   ; dummy seg list 
        jsr     __LibInit(pc)
        tst.l   d0
        beq.s   1$
        
        move.l  d0,-(sp)
        movea.l (4).w,a6
        movea.l d0,a1
        jsr     _LVOAddLibrary(a6)
        move.l  (sp)+,d0

0$      movem.l (sp)+,d1/a0-a2
        rts

1$      movea.l d0,a6
        jsr     __LibExpunge(pc)
        moveq   #0,d0
        bra.s   0$
        rts

;-------------------------------------------------------------------------

        xdef    _ExecCode
        xref    __LibExpunge
        xref    _LIBDoTitle
        xref    _LVOWaitTOF

_ExecCode:
        move.l  d0,d1
        bsr.s   _FalseLib
        tst.l   d0
        bne.s   1$
        moveq   #0,d0
        rts
        
1$      movea.l d0,a6
        move.l  d1,d0
        cmpi.w  #1,d0
        bgt.s   0$
        jsr     _LIBDoPrefs
        bra.s   2$
        
0$      jsr     _LIBDoPlayer

2$      jsr     __LibExpunge(pc)
        bne.s   doend
        move.l  a6,-(sp)
        move.l  ppb_GfxBase,a6
        jsr     _LVOWaitTOF(a6)
        movea.l (sp)+,a6
        bra.s   2$

doend
        moveq   #0,d0
        rts


        END
@
