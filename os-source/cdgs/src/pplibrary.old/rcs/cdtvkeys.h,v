#define RAWKEY_0        0x0F
#define RAWKEY_1        0x1D
#define RAWKEY_2        0x1E
#define RAWKEY_3        0x1F
#define RAWKEY_4        0x2D
#define RAWKEY_5        0x2E
#define RAWKEY_6        0x2F
#define RAWKEY_7        0x3D
#define RAWKEY_8        0x3E
#define RAWKEY_9        0x3F
#define RAWKEY_KPDOT    0x3C
#define RAWKEY_ENTER    0x43

#define RAWKEY_F1       0x50
#define RAWKEY_F2       0x51
#define RAWKEY_F3       0x52
#define RAWKEY_F4       0x53
#define RAWKEY_F5       0x54
#define RAWKEY_F6       0x55
#define RAWKEY_F7       0x56
#define RAWKEY_F8       0x57
#define RAWKEY_F9       0x58
#define RAWKEY_F10      0x59
#define RAWKEY_ESC      0x45
#define RAWKEY_RETURN   0x44
#define RAWKEY_HELP     0x5F

#define RAWKEY_ARROWUP  0x4C
#define RAWKEY_ARROWDN  0x4D
#define RAWKEY_ARROWRT  0x4E
#define RAWKEY_ARROWLT  0x4F

#define RAWKEY_STOP     0x72    /*  Keys on remote controller.  */
#define RAWKEY_PLAY     0x73
#define RAWKEY_PREVTRAK 0x74
#define RAWKEY_NEXTTRAK 0x75
#define RAWKEY_REW      0x76
#define RAWKEY_FF       0x77


#define RAWKEY_LALT     0x64
#define RAWKEY_RALT     0x65
#define RAWKEY_LAMIGA   0x66
#define RAWKEY_RAMIGA   0x67

#define KEY_STOP        RAWKEY_F1
#define KEY_BACK        RAWKEY_F2
#define KEY_PLAY        RAWKEY_F3
#define KEY_FWD         RAWKEY_F4
#define KEY_PAUSE       RAWKEY_F5

#define KEY_TIMEMODE    RAWKEY_F6
#define KEY_SHUFFLE     RAWKEY_F7
#define KEY_GCLR        RAWKEY_F8
#define KEY_INTRO       RAWKEY_F9
#define KEY_REPEAT      RAWKEY_F10

#define KEY_GBUTTON     RAWKEY_KPDOT

#define KEY_CDG         0x41            /* Using the backspace for now */
@
