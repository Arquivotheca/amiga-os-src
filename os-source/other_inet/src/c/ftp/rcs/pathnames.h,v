#ifndef AMIGA
#include <paths.h>
#define	_PATH_FTPUSERS	"/etc/ftpusers"
#else
#define	_PATH_FTPUSERS	"inet:db/ftpusers"
#define _PATH_DEVNULL	"nil:"
#define _PATH_TMP	"t:ftpXXXXXX"
#endif	/* AMIGA */@
