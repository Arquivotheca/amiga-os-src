*------ library dispatch macros --------------------------------------

XLVO	MACRO
	XREF	_LVO\1
	ENDM

CALLLVO	MACRO
		CALLLIB _LVO\1
	ENDM

LINKEXE	MACRO
		LINKLIB _LVO\1,gb_ExecBase(a6)
	ENDM
@
