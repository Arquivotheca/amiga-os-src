set newver rel39_`type graphics_rev.rev`
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd a
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /c
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /d
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /gels
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /text
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /fonts
list `type rcs_link` lformat "rcs -n$newver: %n" | stdin
cd /
