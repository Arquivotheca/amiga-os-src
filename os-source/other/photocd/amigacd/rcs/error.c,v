/****************************************************************************
 *                                                                          *
 *  Includes                                                                *
 *                                                                          *
 ****************************************************************************/

/* Amiga includes */
#include <exec/types.h>

#include <dos/dos.h>

/* ANSI includes */
#include <stdio.h>

/* Local includes */
#include "global.h"

/****************************************************************************
 *                                                                          *
 *  fatalError() - fatal error                                              *
 *                                                                          *
 ****************************************************************************/

void fatalError(struct appContext *appContext,STRPTR message)
{

    printf("%s: %s\n",PROGRAM_NAME,message);

    goodbye(appContext,RETURN_FAIL);

}
