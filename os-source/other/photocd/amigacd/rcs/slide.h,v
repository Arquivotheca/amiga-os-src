#ifndef APP_SLIDE_H
#define APP_SLIDE_H

/****************************************************************************
 *                                                                          *
 *  Slide Node                                                              *
 *                                                                          *
 ****************************************************************************/

struct slideNode {

    /* Header */
    struct MinNode                      sn_Node;    /* Minimal exec.library
                                                       list node */

    /* Properties */
    struct imageNode *                  sn_Image;   /* Image */
    USHORT                              sn_Delay;   /* Delay for this
                                                       slide (seconds) */

};

#endif /* APP_SLIDE_H */
@
